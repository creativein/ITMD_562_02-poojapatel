<% include partials/header%>
<div class="container">
	<h1>Users:</h1>

	<% users.forEach(function(user) { %>
		<li><a href="/reminders"><%= user%></a><br><button>Delete</button></li>
	<% }); %>
	
	<br>


	<form action="/adduser" method="POST" class="form-inline">
	  <div class="form-group">
	    <label for="exampleInputName2">Name</label>
	    <input type="text" class="form-control" id="exampleInputName2"  name="newuser" placeholder="Jane Doe">
	  </div>
	  <div class="form-group">
	    <label for="exampleInputEmail2">Email</label>
	    <input type="email" class="form-control" id="exampleInputEmail2" name="email" placeholder="jane.doe@example.com">
	  </div>
	  <button type="submit" class="btn btn-default">Submit</button>
	</form>
</div>
<% include partials/footer %>